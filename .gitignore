# Ignorar pastas de dependências do Node (Angular)
/node_modules

# Ignorar pastas de dependências do Composer (Laravel)
/legotracker-api/vendor

# Ignorar arquivos de ambiente (Laravel)
/legotracker-api/.env
/legotracker-api/.env.bak

# Ignorar logs e cache do Laravel
/legotracker-api/storage/*.key
/legotracker-api/storage/app/public
/legotracker-api/storage/framework/cache
/legotracker-api/storage/framework/sessions
/legotracker-api/storage/framework/testing
/legotracker-api/storage/framework/views
/legotracker-api/storage/logs

# Ignorar build do Angular (se você for gerar a build e não quiser versionar)
/dist
/build

# Ignorar arquivos de IDEs (opcional, mas recomendado)
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Ignorar arquivos de sistema
.DS_Store
Thumbs.db

# Ignorar arquivos de depuração
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.env.local
.env.development.local
.env.test.local
.env.production.local
.env.staging.local
